{
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "root": "modules/theme",
    "sourceRoot": "modules/theme/src",
    "projectType": "library",
    "generators": {},
    "prefix": "@thiva",
    "tags": [],
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "outputPath": "modules/theme/dist",
                "tsconfig": "modules/theme/tsconfig.json",
                "command": "pnpm build",
                "cwd": "modules/theme"
            }
        },
        "lint": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm lint",
                "cwd": "modules/theme",
                "parallel": false
            }
        },
        "test": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm test",
                "cwd": "modules/theme",
                "parallel": false
            }
        }
    }
}
