{
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "root": "modules/i18n",
    "sourceRoot": "modules/i18n/src",
    "projectType": "library",
    "generators": {},
    "prefix": "@thiva",
    "tags": [],
    "implicitDependencies": [
        "core"
    ],
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "outputPath": "modules/i18n/dist",
                "tsconfig": "modules/i18n/tsconfig.json",
                "command": "pnpm build",
                "cwd": "modules/i18n"
            }
        },
        "lint": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm lint",
                "cwd": "modules/i18n",
                "parallel": false
            }
        },
        "test": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm test",
                "cwd": "modules/i18n",
                "parallel": false
            }
        }
    }
}
